{
  "extends": ["react-app", "airbnb", "airbnb/hooks"],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "no-case-declarations": 0,
    "object-curly-newline": 0,
    "jsx-a11y/label-has-associated-control": 0,
    "comma-dangle": 0
  }
}
